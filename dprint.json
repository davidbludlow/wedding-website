{
  "typescript": {
    "quoteStyle": "preferSingle"
  },
  "json": {
    "associations": [
      "**/*.json",
      "!**/.vscode/extensions.json",
      "!**/.vscode/launch.json",
      "!**/.vscode/settings.json",
      "!**/tsconfig*.json",
      "!**/jsconfig.json"
    ]
  },
  "markdown": {},
  "toml": {},
  "prettier": {
    "associations": [
      "!**/.vscode/extensions.json",
      "!**/.vscode/launch.json",
      "!**/.vscode/settings.json",
      "!**/tsconfig*.json",
      "!**/jsconfig.json"
    ],
    "singleQuote": true,
    "trailingComma": "all"
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json,yaml,yml,md,mdx,toml,vue,html,css,scss,sass,less,hbs,prettierrc,prettierignore,gitignore,graphql,gql}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    ".quasar"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.84.0.wasm",
    "https://plugins.dprint.dev/json-0.17.1.wasm",
    "https://plugins.dprint.dev/markdown-0.15.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/prettier-0.24.0.json@9a57d0d8e440ad90d07a503166af47e7a6a886abd46767933f9c279f72468596"
  ]
}
